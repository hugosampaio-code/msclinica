    },
    {
      "id": "footer_map_section",
      "elType": "section",
      "settings": {
        "layout": "full_width",
        "background_background": "classic",
        "background_color": "#2D2D2D",
        "padding": {
          "unit": "px",
          "top": 30,
          "right": 0,
          "bottom": 30,
          "left": 0
        }
      },
      "elements": [
        {
          "id": "map_container",
          "elType": "container",
          "settings": {
            "content_width": "boxed"
          },
          "elements": [
            {
              "id": "map_placeholder",
              "elType": "widget",
              "widgetType": "text-editor",
              "settings": {
                "editor": "<div style='background-color: #333333; border: 2px dashed #D4A84B; border-radius: 10px; padding: 60px 40px; text-align: center;'><i class='fas fa-map-marked-alt' style='font-size: 60px; color: #D4A84B; margin-bottom: 15px; display: block;'></i><p style='color: #F5F5F5;'>Mapa Google (incorporar iframe com localização da clínica)</p><p style='color: #F5F5F5; font-size: 12px; margin-top: 10px;'>Para adicionar mapa real: Elementor > Google Maps widget</p></div>",
                "text_color": "#F5F5F5"
              }
            }
          ]
        }
      ]
    },
    {
      "id": "footer_copyright_section",
      "elType": "section",
      "settings": {
        "layout": "full_width",
        "background_background": "classic",
        "background_color": "#333333",
        "padding": {
          "unit": "px",
          "top": 25,
          "right": 0,
          "bottom": 25,
          "left": 0
        }
      },
      "elements": [
        {
          "id": "copyright_container",
          "elType": "container",
          "settings": {
            "content_width": "boxed",
            "flex_justify_content": "center"
          },
          "elements": [
            {
              "id": "copyright_text",
              "elType": "widget",
              "widgetType": "text-editor",
              "settings": {
                "editor": "&copy; 2024 MS Clínica - Malheiro & Sampaio. Todos os direitos reservados.",
                "text_color": "#F5F5F5",
                "typography_typography": "custom",
                "typography_font_size": {
                  "unit": "px",
                  "size": 14
                },
                "align": "center"
              }
            }
          ]
        }
      ]
    }
