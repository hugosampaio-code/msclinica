                  "widgetType": "heading",
                  "settings": {
                    "title": "Contactos",
                    "header_size": "h4",
                    "color": "#D4A84B",
                    "typography_typography": "custom",
                    "typography_font_size": {
                      "unit": "px",
                      "size": 18
                    },
                    "typography_text_transform": "uppercase",
                    "typography_letter_spacing": {
                      "unit": "px",
                      "size": 0.5
                    },
                    "margin": {
                      "unit": "px",
                      "bottom": 20
