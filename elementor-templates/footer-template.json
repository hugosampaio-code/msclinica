{
  "version": "1.0.0",
  "title": "MS Clínica - Footer Template",
  "type": "footer",
  "content": [
    {
      "id": "footer_main_section",
      "elType": "section",
      "settings": {
        "layout": "full_width",
        "background_background": "classic",
        "background_color": "#2D2D2D",
        "padding": {
          "unit": "px",
          "top": 60,
          "right": 0,
          "bottom": 40,
          "left": 0
        }
      },
      "elements": [
        {
          "id": "footer_columns",
          "elType": "container",
          "settings": {
            "content_width": "boxed",
            "flex_direction": "row",
            "flex_wrap": "wrap",
            "gap": {
              "unit": "px",
              "size": 40
            }
          },
          "elements": [
            {
              "id": "footer_column_1",
              "elType": "container",
              "settings": {
                "flex_basis": "25%",
                "flex_direction": "column",
                "gap": {
                  "unit": "px",
                  "size": 20
                }
              },
              "elements": [
                {
                  "id": "footer_logo_container",
                  "elType": "container",
                  "settings": {
                    "flex_direction": "row",
                    "flex_align_items": "baseline",
                    "gap": {
                      "unit": "px",
                      "size": 5
                    }
                  },
                  "elements": [
                    {
                      "id": "footer_logo_ms",
                      "elType": "widget",
                      "widgetType": "heading",
                      "settings": {
                        "title": "MS",
                        "header_size": "h3",
                        "color": "#D4A84B",
                        "typography_typography": "custom",
                        "typography_font_size": {
                          "unit": "px",
                          "size": 36
                        },
                        "typography_font_weight": "700"
                      }
                    },
                    {
                      "id": "footer_logo_clinica",
                      "elType": "widget",
                      "widgetType": "heading",
                      "settings": {
                        "title": "Clínica",
                        "header_size": "h3",
                        "color": "#F5F5F5",
                        "typography_typography": "custom",
                        "typography_font_size": {
                          "unit": "px",
                          "size": 20
                        },
                        "typography_font_weight": "700"
                      }
                    }
                  ]
                },
                {
                  "id": "footer_description",
                  "elType": "widget",
                  "widgetType": "text-editor",
                  "settings": {
                    "editor": "Especialistas em Saúde, Beleza e Bem-Estar. O luxo de se sentir bem.",
                    "text_color": "#F5F5F5",
                    "typography_typography": "custom",
                    "typography_font_size": {
                      "unit": "px",
                      "size": 15
                    },
                    "typography_line_height": {
                      "unit": "em",
                      "size": 1.7
                    }
                  }
                },
                {
                  "id": "footer_social_icons",
                  "elType": "widget",
                  "widgetType": "social-icons",
                  "settings": {
                    "social_icon_list": [
                      {
                        "social": "facebook",
                        "link": {
                          "url": "#",
                          "is_external": true
                        }
                      },
                      {
                        "social": "instagram",
                        "link": {
                          "url": "#",
                          "is_external": true
                        }
                      },
                      {
                        "social": "linkedin",
                        "link": {
                          "url": "#",
                          "is_external": true
                        }
                      }
                    ],
                    "shape": "circle",
                    "icon_color": "#FFFFFF",
                    "icon_background_color": "#D4A84B",
                    "icon_background_hover_color": "#B89239",
                    "icon_size": {
                      "unit": "px",
                      "size": 16
                    },
                    "icon_padding": {
                      "unit": "px",
                      "size": 12
                    },
                    "gap": {
                      "unit": "px",
                      "size": 15
                    }
                  }
                }
              ]
            },
            {
              "id": "footer_column_2",
              "elType": "container",
              "settings": {
                "flex_basis": "25%",
                "flex_direction": "column"
