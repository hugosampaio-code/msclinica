                    "icon": "none",
                    "text_color": "#F5F5F5",
                    "text_color_hover": "#D4A84B",
                    "typography_typography": "custom",
                    "typography_font_size": {
                      "unit": "px",
                      "size": 14
                    },
                    "space_between": {
                      "unit": "px",
                      "size": 12
